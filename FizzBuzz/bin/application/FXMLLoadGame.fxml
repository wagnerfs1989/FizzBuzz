<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" styleClass="anchor" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FXMLLoadGameController">
   <children>
      <Label layoutX="51.0" layoutY="191.0" prefHeight="17.0" prefWidth="100.0" text="Nome do jogo:" textFill="#c7e8ee">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <TextField fx:id="textLoad" layoutX="161.0" layoutY="189.0" />
      <Button fx:id="buttonLoad" layoutX="169.0" layoutY="238.0" mnemonicParsing="false" onAction="#actionPlay" text="Carregar" />
      <Label alignment="CENTER" graphicTextGap="5.0" layoutX="82.0" layoutY="90.0" prefHeight="60.0" prefWidth="255.0" text="Carregar" textFill="#ffff00e4">
         <font>
            <Font name="Arial Black" size="35.0" />
         </font>
      </Label>
      <Button fx:id="buttonMenu" layoutX="288.0" layoutY="250.0" mnemonicParsing="false" onAction="#quitButtonPushed" text="Voltar ao menu" />
   </children>
</AnchorPane>
